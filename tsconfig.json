{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "checkJs": false,
    "jsx": "preserve",
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/components/landingPage/Stats.jsx",
    "src/app/components/landingPage/Navbar.jsx",
    "src/app/components/landingPage/Hero.jsx",
    "src/app/components/landingPage/Features.jsx",
    "src/app/page.tsx"
, "src/app/layout.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
